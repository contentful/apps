{
  "name": "vite-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "4.25.0",
    "@contentful/f36-components": "4.65.4",
    "@contentful/f36-tokens": "4.0.2",
