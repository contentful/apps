{
  "name": "@contentful/dam-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-scripts": "1.7.2",
    "@contentful/app-sdk": "^4.17.1",
    "@contentful/dam-app-base": "2.0.0",
    "@contentful/f36-components": "4.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.0"
  },
  "devDependencies": {
    "cross-env": "7.0.3"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "upload": "contentful-app-scripts upload --bundle-dir ./dist",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./dist  --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
