{
  "name": "mockshop-function",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "4.29.6",
    "@contentful/f36-components": "4.79.1",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@tanstack/react-query": "^5.66.11",
    "emotion": "10.0.27",
    "graphql-request": "^7.1.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && npm run build:functions",
    "test": "vitest",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "add-locations": "contentful-app-scripts add-locations",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "build:functions": "contentful-app-scripts build-functions --ci"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "^2.3.0",
    "@contentful/node-apps-toolkit": "^3.12.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.3.1",
    "@tsconfig/recommended": "^1.0.8",
    "@types/node": "^22.13.10",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "7.0.3",
    "typescript": "4.9.5",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
  },
  "homepage": "."
}
