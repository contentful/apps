{
  "name": "potterdb-rest-api-fn-example-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.29.1",
    "@contentful/f36-components": "4.78.0",
    "@contentful/f36-icons": "^4.28.0",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/field-editor-single-line": "^1.4.4",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@tanstack/react-query": "^5.59.0",
    "@vitejs/plugin-react": "^4.3.4",
    "contentful-management": "10.46.4",
    "emotion": "10.0.27",
    "graphql": "16.10.0",
    "graphql-yoga": "4.0.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:functions",
    "test": "vitest",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./dist",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./dist --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "build:functions": "contentful-app-scripts build-functions --ci"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.33.0",
    "@contentful/node-apps-toolkit": "^3.9.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.3.1",
    "@tsconfig/create-react-app": "2.0.5",
    "@types/node": "16.18.126",
    "@types/react": "18.3.13",
    "@types/react-dom": "18.3.1",
    "cross-env": "7.0.3",
    "jsdom": "^26.0.0",
    "prettier": "^3.3.3",
    "typescript": "4.9.5",
    "vite": "^4.0.0",
    "vitest": "^0.24.0"
  },
  "homepage": "."
}
