{
  "name": "potterdb-delivery-fn-example-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.23.0",
    "@contentful/f36-components": "4.52.0",
    "@contentful/f36-tokens": "4.0.2",
    "@contentful/field-editor-single-line": "^1.3.2",
    "@contentful/react-apps-toolkit": "1.2.16",
    "contentful-management": "10.46.0",
    "emotion": "10.0.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build && npm run build:delivery-functions",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "build:delivery-functions": "node build-delivery-functions.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.13.0",
    "@contentful/node-apps-toolkit": "^2.5.2",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@tsconfig/create-react-app": "2.0.1",
    "@types/node": "16.18.53",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "cross-env": "7.0.3",
    "esbuild": "0.19.2",
    "prettier": "^3.0.3",
    "typescript": "4.9.5"
  },
  "homepage": "."
}
