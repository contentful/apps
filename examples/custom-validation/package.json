    "react-dom": "17.0.2",
    "regenerator-runtime": "0.13.9"
  },
  "devDependencies": {
    "@tsconfig/create-react-app": "1.0.2",
    "@types/react": "17.0.41",
    "@types/react-dom": "17.0.14",
