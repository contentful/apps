    "build": "vite build",
    "test": "vitest",
    "eject": "vite eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id ${AMPLITUDE_APP_ID} --token ${CONTENTFUL_CMA_TOKEN}",
