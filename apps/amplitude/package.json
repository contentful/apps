{
  "name": "@contentful/amplitude",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-scripts": "^1.29.0",
    "@contentful/app-sdk": "^4.17.2",
    "@contentful/f36-components": "4.67.0",
    "@contentful/f36-tokens": "4.0.5",
    "@contentful/integration-component-library": "^1.1.41",
