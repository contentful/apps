{
  "name": "sap-commerce",
  "version": "0.2.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@contentful/app-sdk": "^3.42.0",
    "@contentful/ecommerce-app-base": "^1.0.7",
    "@contentful/field-editor-single-line": "^0.13.2",
    "@contentful/field-editor-test-utils": "^0.9.0",
    "@contentful/forma-36-fcss": "^0.3.5",
    "@contentful/forma-36-react-components": "^3.100.7",
    "@contentful/forma-36-tokens": "^0.10.2",
    "@emotion/css": "^11.10.6",
    "cross-env": "^7.0.3",
    "lodash.get": "^4.4.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-sortable-hoc": "^2.0.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-ci": "CI=true react-scripts test",
    "deploy": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${DEFINITIONS_ORG_ID} --definition-id <APP_DEFINITION_ID_HERE> --token ${CONTENTFUL_CMA_TOKEN}",
    "deploy:test": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id ${TEST_ORG_ID} --definition-id ${TEST_APP_ID} --token ${CONTENTFUL_CMA_TOKEN}",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/lodash.get": "^4.4.7",
    "@types/node": "^14.18.41",
    "@types/react": "^17.0.54",
    "@types/react-dom": "^17.0.19",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  }
}
