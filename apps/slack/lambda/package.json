    "@types/chai": "^4.3.11",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.18.12",
    "@types/sinon": "^10.0.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
