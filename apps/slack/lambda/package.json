  "scripts": {
    "build": "rimraf build && tsc",
    "test": "TS_NODE_TRANSPILE_ONLY=1 mocha -r ts-node/register ./lib/**/*.test.ts",
    "lint": "eslint --ext .ts,.js ./",
    "format": "prettier --write '**/*.{jsx,js,ts,tsx}'",
