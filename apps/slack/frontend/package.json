{
  "name": "slack-frontend",
  "version": "0.2.6",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.45.0",
    "@contentful/f36-components": "^5.6.0",
    "@contentful/f36-tokens": "^5.1.0",
    "@contentful/f36-workbench": "^4.21.1",
    "@contentful/react-apps-toolkit": "^1.2.16",
    "@emotion/css": "^11.13.4",
    "contentful-management": "^11.61.0",
    "dotenv": "^16.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^9.0.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.2",
    "zustand": "^4.2.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest",
    "eject": "vite eject",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx,.svg ./",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "deploy": "aws s3 sync ./build ${STATIC_S3_BASE}/slack --acl public-read",
    "deploy:test": "aws s3 sync ./build ${STATIC_S3_BASE}/slack-test --acl public-read",
    "upsert-app-action": "tsx -r dotenv/config scripts/upsertAppAction.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@contentful/app-scripts": "^2.5.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^29.2.5",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^18.0.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^25.0.1",
    "tsx": "^4.20.3",
    "typescript": "4.9.5"
  },
  "homepage": "."
}
