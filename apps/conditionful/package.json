{
  "name": "conditionful",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.29.1",
    "@contentful/f36-components": "4.80.5",
    "@contentful/f36-tokens": "4.2.0",
    "@contentful/field-editor-boolean": "^1.8.0",
    "@contentful/field-editor-date": "^1.9.20",
    "@contentful/field-editor-multiple-line": "^1.7.0",
