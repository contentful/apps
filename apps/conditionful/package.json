    "@contentful/app-scripts": "^2.3.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^22.13.5",
    "@types/react": "18.3.13",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.0.3",
    "cross-env": "7.0.3",
    "jsdom": "^26.0.0",
    "typescript": "4.9.5",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
