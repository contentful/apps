{
  "name": "jira-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@contentful/contentful-extension-scripts": "^0.15.1",
    "@contentful/eslint-config-extension": "0.2.0",
    "@testing-library/react": "8.0.4",
    "@types/jest": "24.0.15",
    "@types/lodash.get": "^4.4.6",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/webpack-env": "1.13.9",
    "contentful-cli": "1.2.12",
    "cssnano": "4.1.10",
    "eslint": "^6.0.1",
    "fetch-mock": "^8.0.1",
    "node-fetch": "^2.6.0",
    "prettier": "1.19.1",
    "sass": "^1.23.7",
    "typescript": "3.5.2"
  },
  "scripts": {
    "start": "contentful-extension-scripts start --serve-only",
    "build": "contentful-extension-scripts build --no-inline",
    "lint:fixme": "eslint ./ --ext .js,.jsx,.ts,.tsx && tsc -p ./ --noEmit",
    "deploy": "npm run build && aws s3 sync ./build s3://cf-apps-jira --acl public-read",
    "test": "contentful-extension-scripts test --env=jsdom",
    "test:ci": "contentful-extension-scripts test --env=jsdom --coverage",
    "test:dev": "contentful-extension-scripts test --env=jsdom --watch",
    "pretty": "prettier --config ./.prettierrc --write '{**/*.tsx,**/*.ts}'"
  },
  "dependencies": {
    "@contentful/forma-36-fcss": "^0.0.20",
    "@contentful/forma-36-react-components": "^3.11.3",
    "@contentful/forma-36-tokens": "^0.3.3",
    "contentful-ui-extensions-sdk": "^3.10.6",
    "lodash.get": "^4.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "browserslist": [
    "last 5 Chrome version",
    "> 1%",
    "not ie <= 11"
  ]
}
