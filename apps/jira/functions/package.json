{
  "name": "@contentful/jira-functions",
  "version": "1.4.107",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "rimraf built && tsc",
    "lint": "tslint --project ./tsconfig.json",
    "lint-fix": "tslint --project ./tsconfig.json --fix",
