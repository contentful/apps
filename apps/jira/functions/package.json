  "dependencies": {
    "aws-sdk": "^2.814.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.36",
    "@types/chai": "^4.2.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.14",
    "@types/node-fetch": "^2.5.4",
    "@types/request": "^2.48.3",
    "@types/sinon-chai": "^3.2.3",
    "mocha": "^10.0.0",
    "nock": "^11.7.0",
    "rimraf": "^3.0.0",
    "serverless": "2.57.0",
    "serverless-domain-manager": "3.3.1",
    "serverless-offline": "^8.8.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "ts-mocha": "^10.0.0",
    "ts-sinon": "^2.0.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.7.2"
  }
