    "@types/request": "^2.48.13",
    "@types/sinon-chai": "^3.2.12",
    "mocha": "^10.8.2",
    "nock": "^11.9.1",
    "rimraf": "^3.0.2",
    "serverless": "^3.31.0",
    "serverless-domain-manager": "3.3.2",
    "serverless-offline": "^8.8.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "ts-mocha": "^10.1.0",
    "ts-sinon": "^2.0.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
