{
  "name": "gatsby-preview",
  "version": "1.10.0",
  "private": true,
  "devDependencies": {
    "@contentful/app-scripts": "0.8.11",
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/react": "8.0.7",
    "cross-env": "7.0.3"
  },
  "dependencies": {
    "@contentful/app-sdk": "4.2.1",
    "@contentful/forma-36-fcss": "0.3.3",
    "@contentful/forma-36-react-components": "3.93.2",
    "@contentful/forma-36-tokens": "0.11.0",
    "@gatsby-cloud-pkg/gatsby-cms-extension-base": "0.0.49",
    "emotion": "10.0.14",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
