    "node-fetch": "^2.6.1",
    "openid-client": "^4.9.1",
    "rimraf": "^3.0.1",
    "serverless": "^4.4.7",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/node": "^12.20.55",
    "@types/node-fetch": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^8.57.1",
    "serverless-domain-manager": "^3.3.0",
    "serverless-express": "^2.0.12",
    "serverless-prune-plugin": "^2.0.2",
    "supertest": "^4.0.2",
