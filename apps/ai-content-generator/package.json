{
  "name": "ai-content-generator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "4.22.1",
    "@contentful/f36-components": "4.45.0",
    "@contentful/f36-tokens": "4.0.2",
    "@contentful/react-apps-toolkit": "1.2.16",
    "@contentful/rich-text-plain-text-renderer": "^16.0.5",
    "contentful-management": "10.28.1",
    "emotion": "10.0.27",
    "openai": "^3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "start": "vite",
    "start:dist": "http-server dist -p 3000 -c-1",
    "build": "tsc && vite build",
    "test": "vitest",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./dist",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./dist  --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.7.13",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "18.0.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@vitejs/plugin-react": "1.3.2",
    "happy-dom": "6.0.2",
    "http-server": "^14.1.1",
    "typescript": "4.7.4",
    "vite": "3.2.7",
    "vitest": "0.18.0"
  }
}
